pipeline {
    agent any
    stages {
        stage('Info') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Hello World'
                sh 'sleep 6'
            }
        }
         stage('Test') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello World'
            }
        }
        
    }
}
